<div class="col-md-12" data-id="0">
    <div class="newsContainer">
        <div class="newsItem">
            <div class="newsTitle">
                <div class="newsAuthor">
                    <img class="newsAuthorImg" src="style/nophoto.png">
                    <span class="hidden-xs">Новость создал </span>
                    <span class="newsAutorName">{login}</span>
                    <span class="newsTimeCreate">{timestamp}</span>
                </div>

                <div class="newsApproved text-right hidden-xs hidden">
                    <span>Принято: </span>
                    <img class="newsAuthorImg" src="style/nophoto.png">
                    <span class="newsAutorName">MSN</span>
                    <span class="newsTimeCreate">22.04.2017 в 02:34</span>
                </div>

                <div class="btnApproved text-right hidden">
                    <button class="btnAddNews btn btn-success btn-xs disabled">Принимаю</button>
                </div>
            </div>
            <div class="newsBody">
                <div class="row">
                    <div class="col-md-4 newsLeft">
                        <img src="style/nophoto.png" class="newsImg">
                        <div class="newsImgDesc"></div>
                    </div>
                    <div class="col-md-8 newsRight">
                        <h2 class="newsTitleH">{newsTitle}</h2>
                        <span class="newsText">{newsText}</span>
                    </div>
                </div>
            </div>
            <div class="newsFooter"></div>
        </div>
    </div>
</div>